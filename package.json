{
  "name": "mokamok",
  "version": "0.1.0",
  "description": "Zero configuration JavaScript Unit Testing with Mocha, Chai and Sinon",
  "repository": {
    "type": "git",
    "url": "https://github.com/ggyorfi/mokamok"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "node tests",
    "build": "babel src --presets latest --out-dir lib --copy-files",
    "watch": "babel src --presets latest --out-dir lib --copy-files --source-maps inline --watch",
    "prepublish": "npm run build"
  },
  "bin": {
    "mokamok": "bin/mokamok.js"
  },
  "author": "Gabor Gyorfi",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-latest": "^6.14.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "chokidar": "^1.6.0",
    "commander": "^2.9.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.4.5",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai-jquery": "^2.0.0",
    "chalk": "^1.1.3",
    "diff": "^3.0.0",
    "fs-extra": "^0.30.0",
    "jquery": "^3.1.1",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "zepto": "^1.2.0",
    "zepto-node": "^1.0.0"
  }
}
